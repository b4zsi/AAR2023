<header class="mb-7 pb-1">
    <div>
        <div class="bg-punch-light">
            <div class="container py-3 py-md-4">
                <div class="d-flex align-items-center position-relative flex-wrap">
                    <div class="d-none d-xl-flex align-items-center mt-3 mt-md-0 mr-md-auto">

                        <a href="mailto:info@oazis.com" class="mr-4 mb-4 mb-md-0">
                            <div class="d-flex align-items-center text-dark font-size-2 text-lh-sm">
                                <i class="flaticon-question font-size-5 mt-2 mr-1"></i>
                                <div class="ml-2">
                                    <span class="text-secondary-gray-1090 font-size-1">info@oazis.com</span>
                                    <div class="h6 mb-0">Küldj e-mailt!</div>
                                </div>
                            </div>
                        </a>


                        <a href="tel:+36 30 999 9999">
                            <div class="d-flex align-items-center text-dark font-size-2 text-lh-sm">
                                <i class="flaticon-phone font-size-5 mt-2 mr-1"></i>
                                <div class="ml-2">
                                    <span class="text-secondary-gray-1090 font-size-1">+36 30 999 9999</span>
                                    <div class="h6 mb-0">Hívjon minket!</div>
                                </div>
                            </div>
                        </a>

                    </div>
                    <div class="site-branding pr-md-7 mx-auto mx-md-0">
